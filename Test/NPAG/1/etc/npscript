cd '/Users/Neely/LAPK/PmetricsSource/Test/NPAG'
echo Unix>time.txt
date +%s>>time.txt
./np_prep MacOSX < PMcontrol
echo 1 > extnum
echo go > go
gfortran -m64 -w -O3 -o np_run '/Users/Neely/.config/Pmetrics/compiledFortran/sNPeng.o' npagdriv.f
./np_run < go
echo;echo Cleaning up....;echo
stty -echo
mkdir 1
mkdir 1/inputs
mkdir 1/outputs
mkdir 1/wrkcopy
mkdir 1/etc
echo ex.csv >> NP_RF0001.TXT
if [ ! -f NP_RF0001.TXT ]; then error=true; else error=false; fi
if [ -f DEN* ]; then mv DEN* 1/outputs; fi
if [ -f OUT0* ]; then mv OUT0* 1/outputs; fi
if [ -f OUTT* ]; then mv OUTT* 1/outputs; fi
if [ -f PRTB* ]; then mv PRTB* 1/outputs; fi
if [ -f ILOG* ]; then mv ILOG* 1/outputs; fi
if [ -f NP_RF* ]; then mv NP_RF* 1/outputs; fi
if [ -f ERROR* ]; then mv ERROR* 1/outputs; fi
mv instr.inx 1/etc
mv log.txt 1/outputs
mv PMcontrol 1/etc
mv model.for 1/etc/model.for
mv model.txt 1/inputs/model.txt
mv XQZPJ*.ZMQ 1/wrkcopy
mv extnum 1/etc
mv npag*.* 1/etc
rm CHMAX*.*
if [ -f FROM0001 ]; then mv FROM0001 1/inputs; fi
rm fort.*
rm go
mv np_prep* 1/etc
mv np_run* 1/etc
mv ex.csv 1/inputs
date +%s >> time.txt
mv time.txt 1/outputs
if ! $error ; then 
/Library/Frameworks/R.framework/Versions/3.2/Resources/bin/Rscript '/Library/Frameworks/R.framework/Versions/3.2/Resources/library/Pmetrics/report/NPrepScript.R' '/Users/Neely/LAPK/PmetricsSource/Test/NPAG/1/outputs' median FALSE
open '/Users/Neely/LAPK/PmetricsSource/Test/NPAG/1/outputs/NPAGreport.html' ; fi
mv npscript 1/etc
