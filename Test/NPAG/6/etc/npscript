cd '/Users/Neely/LAPK/PmetricsSource/Test/NPAG'
echo Unix>time.txt
date +%s>>time.txt
./np_prep MacOSX < PMcontrol
echo 1 > extnum
echo go > go
gfortran -O3 -w -fopenmp -fmax-stack-var-size=32768 -o np_run '/Users/Neely/.config/Pmetrics/compiledFortran/pNPeng.o' npagdriv.f
./np_run < go
echo;echo Cleaning up....;echo
stty -echo
mkdir 6
mkdir 6/inputs
mkdir 6/outputs
mkdir 6/wrkcopy
mkdir 6/etc
echo ex.csv >> NP_RF0001.TXT
if [ ! -f NP_RF0001.TXT ]; then error=true; else error=false; fi
if [ -f DEN* ]; then mv DEN* 6/outputs; fi
if [ -f OUT0* ]; then mv OUT0* 6/outputs; fi
if [ -f OUTT* ]; then mv OUTT* 6/outputs; fi
if [ -f PRTB* ]; then mv PRTB* 6/outputs; fi
if [ -f ILOG* ]; then mv ILOG* 6/outputs; fi
if [ -f NP_RF* ]; then mv NP_RF* 6/outputs; fi
if [ -f ERROR* ]; then mv ERROR* 6/outputs; fi
mv instr.inx 6/etc
mv log.txt 6/outputs
mv PMcontrol 6/etc
mv model.for 6/etc/model.for
mv model.txt 6/inputs/model.txt
mv XQZPJ*.ZMQ 6/wrkcopy
mv extnum 6/etc
mv npag*.* 6/etc
rm CHMAX*.*
if [ -f FROM0001 ]; then mv FROM0001 6/inputs; fi
rm fort.*
rm go
mv np_prep* 6/etc
mv np_run* 6/etc
mv ex.csv 6/inputs
date +%s >> time.txt
mv time.txt 6/outputs
if ! $error ; then 
/Library/Frameworks/R.framework/Versions/3.2/Resources/bin/Rscript '/Library/Frameworks/R.framework/Versions/3.2/Resources/library/Pmetrics/report/NPrepScript.R' '/Users/Neely/LAPK/PmetricsSource/Test/NPAG/6/outputs' median TRUE
open '/Users/Neely/LAPK/PmetricsSource/Test/NPAG/6/outputs/NPAGreport.html' ; fi
mv npscript 6/etc
