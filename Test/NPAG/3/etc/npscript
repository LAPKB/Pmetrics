cd '/Users/Neely/LAPK/PmetricsSource/Test/NPAG'
echo Unix>time.txt
date +%s>>time.txt
./np_prep MacOSX < PMcontrol
echo 1 > extnum
echo go > go
gfortran -m64 -w -O3 -o np_run '/Users/Neely/.config/Pmetrics/compiledFortran/sNPeng.o' npagdriv.f
./np_run < go
echo;echo Cleaning up....;echo
stty -echo
mkdir 3
mkdir 3/inputs
mkdir 3/outputs
mkdir 3/wrkcopy
mkdir 3/etc
echo ex.csv >> NP_RF0001.TXT
if [ ! -f NP_RF0001.TXT ]; then error=true; else error=false; fi
if [ -f DEN* ]; then mv DEN* 3/outputs; fi
if [ -f OUT0* ]; then mv OUT0* 3/outputs; fi
if [ -f OUTT* ]; then mv OUTT* 3/outputs; fi
if [ -f PRTB* ]; then mv PRTB* 3/outputs; fi
if [ -f ILOG* ]; then mv ILOG* 3/outputs; fi
if [ -f NP_RF* ]; then mv NP_RF* 3/outputs; fi
if [ -f ERROR* ]; then mv ERROR* 3/outputs; fi
mv instr.inx 3/etc
mv log.txt 3/outputs
mv PMcontrol 3/etc
mv model.for 3/etc/model.for
mv model.txt 3/inputs/model.txt
mv XQZPJ*.ZMQ 3/wrkcopy
mv extnum 3/etc
mv npag*.* 3/etc
rm CHMAX*.*
if [ -f FROM0001 ]; then mv FROM0001 3/inputs; fi
rm fort.*
rm go
mv np_prep* 3/etc
mv np_run* 3/etc
mv ex.csv 3/inputs
date +%s >> time.txt
mv time.txt 3/outputs
if ! $error ; then 
/Library/Frameworks/R.framework/Versions/3.2/Resources/bin/Rscript '/Library/Frameworks/R.framework/Versions/3.2/Resources/library/Pmetrics/report/NPrepScript.R' '/Users/Neely/LAPK/PmetricsSource/Test/NPAG/3/outputs' median FALSE
open '/Users/Neely/LAPK/PmetricsSource/Test/NPAG/3/outputs/NPAGreport.html' ; fi
mv npscript 3/etc
