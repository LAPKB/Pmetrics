cd '/Users/Neely/LAPK/PmetricsSource/Test/NPAG'
echo Unix>time.txt
date +%s>>time.txt
./np_prep MacOSX < PMcontrol
echo 1 > extnum
echo go > go
gfortran -m64 -w -O3 -o np_run '/Users/Neely/.config/Pmetrics/compiledFortran/sNPeng.o' npagdriv.f
./np_run < go
echo;echo Cleaning up....;echo
stty -echo
mkdir 2
mkdir 2/inputs
mkdir 2/outputs
mkdir 2/wrkcopy
mkdir 2/etc
echo ex.csv >> NP_RF0001.TXT
if [ ! -f NP_RF0001.TXT ]; then error=true; else error=false; fi
if [ -f DEN* ]; then mv DEN* 2/outputs; fi
if [ -f OUT0* ]; then mv OUT0* 2/outputs; fi
if [ -f OUTT* ]; then mv OUTT* 2/outputs; fi
if [ -f PRTB* ]; then mv PRTB* 2/outputs; fi
if [ -f ILOG* ]; then mv ILOG* 2/outputs; fi
if [ -f NP_RF* ]; then mv NP_RF* 2/outputs; fi
if [ -f ERROR* ]; then mv ERROR* 2/outputs; fi
mv instr.inx 2/etc
mv log.txt 2/outputs
mv PMcontrol 2/etc
mv model.for 2/etc/model.for
mv model.txt 2/inputs/model.txt
mv XQZPJ*.ZMQ 2/wrkcopy
mv extnum 2/etc
mv npag*.* 2/etc
rm CHMAX*.*
if [ -f FROM0001 ]; then mv FROM0001 2/inputs; fi
rm fort.*
rm go
mv np_prep* 2/etc
mv np_run* 2/etc
mv ex.csv 2/inputs
date +%s >> time.txt
mv time.txt 2/outputs
if ! $error ; then 
/Library/Frameworks/R.framework/Versions/3.2/Resources/bin/Rscript '/Library/Frameworks/R.framework/Versions/3.2/Resources/library/Pmetrics/report/NPrepScript.R' '/Users/Neely/LAPK/PmetricsSource/Test/NPAG/2/outputs' median FALSE
open '/Users/Neely/LAPK/PmetricsSource/Test/NPAG/2/outputs/NPAGreport.html' ; fi
mv npscript 2/etc
