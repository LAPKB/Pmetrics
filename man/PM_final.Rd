\deqn{popVar = var(EBE) + mean(var(EBD))}
\deqn{1 = \frac{var(EBE)}{popVar} + \frac{mean(var(EBD)}{popVar}}
\deqn{1 - \frac{var(EBE)}{popVar} = \frac{mean(var(EBD))}{popVar}}
\deqn{shrinkage = \frac{mean(var(EBD))}{popVar}}
